{{#if long}}
    {{#each carrito}}
    <div class="productoCarro">
        <h2>{{this.title}}</h2>
        <h2>{{this.price}}</h2>
        <h2>{{this.thumbnail}}</h2>
        <button id="{{this.idUsers}}-{{this.id}}-{{this.idSet}}" data-user="{{user}}" data-pass="{{pass}}">Eliminar del carro</button>
    </div>
    {{/each}}
{{else}}
<h2>No hay productos</h2>
{{/if}}
<script src="../../js/carrito.js"></script>